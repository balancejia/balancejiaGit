<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
<item android:id="@android:id/background">
<shape>
<corners android:radius="8.0dip" />
<gradient android:startColor="@color/bikesite_orange" android:endColor="@color/bikesite_orange"
android:angle="270.0" />
</shape>
</item>
<item android:id="@android:id/progress">
<clip>
<shape>
<corners android:radius="8.0dip" />
<gradient android:startColor="@color/bikesite_blue" android:endColor="@color/bikesite_blue"
android:centerColor="@color/bikesite_blue" android:angle="270.0" />
</shape>
</clip>
</item>
<item android:id="@android:id/secondaryProgress">
<clip>
<shape>
<corners android:radius="8.0dip" />
<gradient android:startColor="@color/bikesite_blue" android:endColor="@color/bikesite_blue"
android:centerColor="@color/bikesite_blue" android:angle="270.0" />
</shape>
</clip>
</item>
</layer-list>